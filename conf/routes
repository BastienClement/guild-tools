# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET         /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET			/gtp3						controllers.Application.gtp3

GET         /api/gamedata               controllers.Api.gamedata
POST        /api/bugsack                controllers.Api.bugsack
GET         /api/socket_url             controllers.Api.socketURL
GET         /api/*path                  controllers.Api.catchall(path)

GET         /ping/dashboard/feed        controllers.Ping.dashboardFeed

GET         /                           controllers.Application.client
GET         /unsupported                controllers.Application.unsupported
GET         /*path                      controllers.Application.catchall(path)
